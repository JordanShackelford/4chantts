{
  "name": "4chan TTS Reader",
  "short_name": "4chan TTS",
  "description": "Text-to-speech reader for 4chan threads with background audio support",
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#007AFF",
  "background_color": "#1a1a1a",
  "scope": "/",
  "lang": "en",
  "categories": ["entertainment", "productivity", "utilities"],
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "320x568",
      "type": "image/svg+xml",
      "form_factor": "narrow"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "shortcuts": [
    {
      "name": "Random Thread",
      "short_name": "Random",
      "description": "Load a random 4chan thread",
      "url": "/?random=true",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+4chan",
      "url": "/?thread=%s"
    }
  ],
  "file_handlers": [],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "display_override": ["window-controls-overlay", "minimal-ui"],
  "id": "4chan-tts-reader"
}